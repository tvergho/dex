{
  "name": "agentdex",
  "version": "0.3.5",
  "description": "Local search engine for AI coding agent conversations (Cursor, Claude Code, Codex, OpenCode)",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvergho/agentdex.git"
  },
  "homepage": "https://agentdex.sh",
  "bugs": {
    "url": "https://github.com/tvergho/agentdex/issues"
  },
  "keywords": [
    "cli",
    "search",
    "ai",
    "agents",
    "cursor",
    "claude",
    "codex",
    "opencode",
    "conversations"
  ],
  "bin": {
    "dex": "./bin/dex"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=18 <23"
  },
  "scripts": {
    "dev": "LANCEDB_LOG=error tsx src/index.ts",
    "dev:bun": "LANCEDB_LOG=error bun run src/index.ts",
    "build": "tsup src/index.ts --format esm --target node18 --shims",
    "prepublishOnly": "bun run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:cursor": "node --import tsx --test tests/unit/adapters/cursor.node-test.ts",
    "test:all": "bun test && npm run test:cursor",
    "reset": "rm -rf ~/.dex 2>/dev/null; echo 'âœ“ Data directory reset'"
  },
  "dependencies": {
    "@lancedb/lancedb": "0.22.3",
    "@modelcontextprotocol/sdk": "1.24.3",
    "@openauthjs/openauth": "0.4.3",
    "@opencode-ai/sdk": "1.0.123",
    "apache-arrow": "18.1.0",
    "better-sqlite3": "11.10.0",
    "chalk": "5.6.2",
    "commander": "12.1.0",
    "fullscreen-ink": "0.1.0",
    "ink": "5.2.1",
    "marked": "17.0.1",
    "marked-terminal": "7.3.0",
    "opencode-ai": "1.0.123",
    "react": "18.3.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.6.13",
    "@types/bun": "1.3.3",
    "@types/marked-terminal": "6.1.1",
    "@types/react": "18.3.27",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "eslint": "9.39.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "tsup": "8.5.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
}
